{
  "name": "motomanager",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.2.1",
  "scripts": {
    "build": "react-router build && cp -r app/db/migrations build/server/",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "studio": "drizzle-kit studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint . --ext .ts,.tsx",
    "db:migrate": "ts-node ./scripts/migrate.ts",
    "approve-builds": "pnpm approve-builds",
    "release-it": "release-it",
    "repomix": "repomix --config repomix.config.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@libsql/client": "^0.15.15",
    "@react-router/node": "^7.9.4",
    "@react-router/serve": "^7.9.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.17",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "isbot": "^5.1.31",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-easy-crop": "^5.5.5",
    "react-router": "^7.9.4",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@csmith/release-it-calver-plugin": "^2023.12.0",
    "@favoloso/conventional-changelog-emoji": "^0.10.0",
    "@playwright/test": "^1.56.1",
    "@react-router/dev": "^7.9.4",
    "@release-it/conventional-changelog": "^10.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.38.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "jsdom": "^27.0.1",
    "release-it": "^19.0.5",
    "repomix": "^1.9.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild"
    ]
  },
  "version": "2025.11.0"
}
