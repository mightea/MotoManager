{
  "name": "motomanager",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.2.1",
  "scripts": {
    "build": "react-router build && cp -r app/db/migrations build/server/",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "studio": "drizzle-kit studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "oxlint .",
    "db:migrate": "ts-node ./scripts/migrate.ts",
    "approve-builds": "pnpm approve-builds",
    "release-it": "release-it",
    "repomix": "repomix --config repomix.config.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@libsql/client": "^0.17.0",
    "@react-router/node": "^7.13.0",
    "@react-router/serve": "^7.13.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/vite": "^4.1.18",
    "better-sqlite3": "^12.6.2",
    "clsx": "^2.1.1",
    "dotenv": "^17.3.1",
    "drizzle-orm": "^0.45.1",
    "isbot": "^5.1.35",
    "lucide-react": "^0.564.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-dropzone": "^15.0.0",
    "react-easy-crop": "^5.5.6",
    "react-router": "^7.13.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@csmith/release-it-calver-plugin": "^2023.12.0",
    "@favoloso/conventional-changelog-emoji": "^0.10.0",
    "@playwright/test": "^1.58.2",
    "@react-router/dev": "^7.13.0",
    "@release-it/conventional-changelog": "^10.0.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "drizzle-kit": "^0.31.9",
    "jsdom": "^28.1.0",
    "oxlint": "^1.49.0",
    "release-it": "^19.2.4",
    "repomix": "^1.11.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-babel": "^1.5.1",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild"
    ]
  },
  "version": "2026.2.3"
}
